<% module_namespacing do -%>
class <%= class_name %>Resource < ApplicationResource
  <%- if attributes.empty? -%>
  default_attributes
  <%- else -%>
  <%- attributes.each do |a| -%>
  <%- if [:created_at, :updated_at].include?(a.name.to_sym) -%>
  attribute :<%= a.name %>, :<%= a.type %>, writable: false
  <%- else -%>
  attribute :<%= a.name %>, :<%= a.type %>
  <%- end -%>
  <%- end -%>
  <%- end -%>
end
<% end -%>
